    "/* The program shows the area of triangle with sides a , b and c*/\n",
    "\n",
    "int a=4,b=6,c=8;\n",
    "double s=((a+b+c)/2);\n",
    "\n",
    "double area=Math.Sqrt(s*(s-a)*(s-b)*(s-c));\n",
    "\n",
    "Console.WriteLine(area);"
   ]
